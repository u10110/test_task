datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int        @id @default(autoincrement())
  username String     @unique
  password String
  products Products[]
}

model Products {
  id          Int    @id @default(autoincrement())
  title       String
  description String
  price       String
  cont        Int
  user        User   @relation(fields: [userId], references: [id])
  userId      Int
}
